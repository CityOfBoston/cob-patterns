{
  "name": "cob-patterns",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/CityOfBoston/patterns.git"
  },
  "scripts": {
    "dev": "concurrently \"gulp\" \"fractal start --watch\" \"stencil build --docs --prerender --watch\"",
    "dev-chrome": "concurrently \"gulp\" \"fractal start --watch\" \"stencil build --docs --dev --prerender --watch\"",
    "build": "stencil build --prerender --docs && gulp build && fractal build",
    "fractal-build": "fractal build",
    "gulp-build": "gulp build",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .tsx,.ts,.js .",
    "test": "npm run jest && npm run testcafe",
    "jest": "jest",
    "jest.ci": "jest",
    "jest.dev": "jest --watch",
    "testcafe": "npm run build && npm run testcafe.local",
    "testcafe.local": "cross-env TEST_SERVER_URL=http://localhost:3035/ testcafe -a 'http-server -r public -p 3035' -c 3 chrome:headless components/**/*.testcafe.*",
    "testcafe.dev": "testcafe -L remote components/**/*.testcafe.*",
    "testcafe.ci": "cross-env BROWSERSTACK_USE_AUTOMATE=1 TEST_SERVER_URL=http://localhost:3035/ testcafe -a 'http-server -r public -p 3035' 'browserstack:edge@18,browserstack:ie@11.0:Windows 8.1,browserstack:chrome:Windows 10,browserstack:firefox@64:Windows 10' components/**/*.testcafe.*",
    "precommit": "lint-staged",
    "prepush": "jest --clearCache && npm run test",
    "preinstall": "npx npm-force-resolutions"
  },
  "jest": {
    "transform": {
      "\\/web-components\\/.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "\\.spec\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --quiet",
    "*.svg": [
      "svgo --config svgo.yml",
      "git add"
    ]
  },
  "dependencies": {
    "@frctl/fractal": "^1.2.1",
    "@stencil/core": "^1.13.0",
    "@types/core-js": "2.5.0",
    "@types/d3-fetch": "1.1.2",
    "@types/geojson": "7946.0.1",
    "@types/jest": "22.1.1",
    "@types/leaflet": "1.2.5",
    "ajv": "^5.5.2",
    "autoprefixer": "^6.7.7",
    "concurrently": "^3.6.1",
    "core-js": "^2.6.11",
    "d3-fetch": "1.1.2",
    "docson": "^2.1.0",
    "esri-leaflet": "2.2.3",
    "esri-leaflet-geocoder": "2.2.13",
    "graceful-fs": "4.2.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-load-plugins": "^2.0.3",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^3.0.2",
    "handlebars": "^4.7.6",
    "laggard": "^0.1.0",
    "leaflet": "1.3.2",
    "leaflet.markercluster": "github:CityOfBoston/Leaflet.markercluster#160f1f0",
    "lodash": "^4.17.15",
    "lost": "^8.3.1",
    "postcss": "^6.0.23",
    "postcss-flexibility": "^1.1.0",
    "poststylus": "^0.2.3",
    "require-dir": "^0.3.0",
    "rucksack-css": "^0.9.1",
    "run-sequence": "^1.2.2",
    "serve-static": "^1.14.1",
    "templayed": "github:CityOfBoston/templayed.js",
    "vega": "^5.11.1",
    "vega-lib": "4.2.0",
    "vega-lite": "^4.12.0",
    "vega-tooltip": "0.17.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "escape-html": "^1.0.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^2.7.0",
    "gulp-plumber": "^1.2.1",
    "gulp-util": "^3.0.8",
    "http-server": "^0.12.3",
    "husky": "^0.14.3",
    "jest": "^26.0.1",
    "json-schema-to-typescript": "^5.7.0",
    "lint-staged": "^6.1.1",
    "mockdate": "^2.0.5",
    "plugin-error": "^1.0.1",
    "prettier": "^1.19.1",
    "svgo": "^1.3.2",
    "testcafe": "^15.1.317922",
    "testcafe-browser-provider-browserstack": "^1.12.0",
    "through2": "^2.0.5",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^12.0.0",
    "web-server-cli": "^2.0.2"
  },
  "resolutions": {
    "graceful-fs": "4.2.3"
  }
}
